import java.util.Scanner;

public class Task1 {
	
	public void enroll() {

		

	}

	public static void main(String[] args) {
	
		System.out.println("Enter course to enroll('Done' to quit)");
		System.out.println("Courses offered are: Hist 101, MTH 101, Eng 101, CHM 101, and CSC 101");
		Scanner in = new Scanner(System.in);
		String[] courses = new String[5];
		
		int costs=0;
		for(int i=0;i<courses.length;i++) {
			courses[i]=in.next();
			costs+=600;
		}

		in.close();
		System.out.println(String.valueOf(courses)+ "  "+ costs);
}
}